{
  "title": "Storage Event Function App (SCALER)",
  "name":"storage-event-function-app",
  "duration": 5,
  "documentation_link": "https://cloud.google.com/shell/docs/cloud-shell-tutorials/deploystack/storage-event-function-app",
  "collect_project": true,
  "collect_project_number": true,
  "collect_region": true,
  "region_type": "run",
  "region_default": "us-central1",
  "hard_settings": {
    "basename": "scaler"
  },
  "custom_settings": [
    {
      "name": "bucket",
      "description": "the name of the Google Cloud Bucket to use in this application",
      "default": "bucket",
      "prepend_project": true
    },
    {
      "name": "location",
      "description": "the location for the Cloud Storage Bucket",
      "default": "US",
      "options": ["US", "EU", "ASIA"]
    }
  ]
}
